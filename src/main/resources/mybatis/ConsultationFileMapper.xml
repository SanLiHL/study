<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.springboot.consultation.mapper.ConsultationFileMapper">

    <resultMap id="consultationFileMap" type="com.study.springboot.consultation.entity.ConsultationContextPhoto">
        <id column="id" property="id" ></id>
        <result column="file_type" property="fileType"></result>
        <result column="file_name" property="fileName"></result>
        <result column="file_path" property="filePath"></result>
        <result column="consultation_no" property="consultationNo"></result>
        <result column="create_time" property="createTime"></result>
        <result column="modify_time" property="modifyTime"></result>
        <result column="state" property="state"></result>
    </resultMap>

    <insert id="insert" parameterType="com.study.springboot.consultation.entity.Consultation">
        <selectKey resultType="java.lang.Integer" keyProperty="id" order="AFTER" >
            SELECT LAST_INSERT_ID()
        </selectKey>
        insert into ConsultationFiles (file_type,file_name,file_path,consultation_no,create_time,modify_time,state) values (#{name},#{sex},#{cardId})

    </insert>

    <insert id="insertList" parameterType="list">
        INSERT INTO consultationfiles
        (id, file_type, file_name, file_path, consultation_no, create_time, modify_time, state)
        VALUES
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.id},#{item.fileType},#{item.fileName},#{item.filePath},#{item.consultationNo},#{item.createTime},#{item.modifyTime},#{item.state})
        </foreach>
    </insert>

</mapper>